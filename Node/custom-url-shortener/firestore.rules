rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    
    match /dinoFacts/{fact} {
    	allow read: if true;
    }
    
    match /users/{userId}/boardChanges/{changeId} {
    	allow read, write: if true;
    }
    
    match /canvas/emoji {
    	allow read: if true;
    }
  }
}